<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.shianxian.supervise.cockpit.dao.CockpitPlanTaskDao">

    <resultMap id="cockpitPlanSuperviseTypeCircleVO" type="cn.shianxian.supervise.cockpit.vo.CockpitPlanSuperviseTypeCircleVO"></resultMap>
    <resultMap id="cockpitPlanAreaBarVO" type="cn.shianxian.supervise.cockpit.vo.CockpitPlanAreaBarVO"></resultMap>
    <resultMap id="cockpitPlanDistrictBarVO" type="cn.shianxian.supervise.cockpit.vo.CockpitPlanDistrictBarVO"></resultMap>

    <select id="selectCockpitPlan" resultMap="cockpitPlanSuperviseTypeCircleVO,cockpitPlanAreaBarVO,cockpitPlanDistrictBarVO">
        CALL Proc_jsc_QueryPlan()
    </select>

    <select id="selectAllArea" resultType="cn.shianxian.supervise.cockpit.vo.CockpitAreaVO">
        CALL Proc_jsc_QueryAllArea()
    </select>

    <select id="selectAllSuperviseType" resultType="cn.shianxian.supervise.cockpit.vo.CockpitSuperviseTypeVO">
        CALL Proc_jsc_QueryAllSuperviseType()
    </select>

    <select id="selectCockpitPlanComposite" resultType="cn.shianxian.supervise.cockpit.vo.CockpitPlanAreaSuperviseTypeCompositeVO">
        CALL Proc_jsc_QueryPlanComposite
        (
        #{id}
        )
    </select>

    <select id="selectCockpitPlanAreaLine" resultType="int">
        CALL Proc_jsc_QueryPlanLine_ByAreaTag
        (
        #{id}
        )
    </select>

    <select id="selectCockpitPlanSuperviseTypeLine" resultType="int">
        CALL Proc_jsc_QueryPlanLine_BySuperviseTypeTag
        (
        #{id}
        )
    </select>
</mapper>
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.shianxian.supervise.info.dao.SuperviseInfoMainTypeDao">

    <select id="saveSuperviseInfoMainType" resultType="java.lang.String">
        CALL proc_T_Supervise_Info_Main_Type_Add
        (
        #{superviseInfoMainType.mainIds},
        #{superviseInfoMainType.mainId},
        #{superviseInfoMainType.nodeTag},
        #{superviseInfoMainType.superviseTypeTag},
        #{superviseInfoMainType.superviseTeamTag},
        #{superviseInfoMainType.superviseTeamName},
        #{superviseInfoMainType.superviserTag},
        #{superviseInfoMainType.superviserName},
        #{superviseInfoMainType.functionaryTag},
        #{superviseInfoMainType.functionaryName},
        #{superviseInfoMainType.parentMainIds}
        )
    </select>

    <select id="updateSuperviseInfoMainType" resultType="java.lang.String">
        CALL proc_T_Supervise_Info_Main_update_ByMainID(#{mainIds})
    </select>

</mapper>
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.shianxian.supervise.info.dao.SuperviseInfoSubOldDao">

    <select id="selectSuperviseInfoMainOldByLike" resultType="cn.shianxian.supervise.info.pojo.SuperviseInfoSubOld">
        CALL proc_T_Supervise_Info_Old_Query_Detail
        (
        #{superviseInfoSubOld.mainId},
        #{superviseInfoSubOld.superviseTypeTag}
        )
    </select>

    <select id="saveSuperviseInfoSubOld" resultType="java.lang.String">
        CALL proc_T_Supervise_Info_Old_Sub_Add
        (
        #{superviseInfoSubOld.mainId},
        #{superviseInfoSubOld.nodeTag},
        #{superviseInfoSubOld.superviseTypeTag},
        #{superviseInfoSubOld.superviseTag},
        #{superviseInfoSubOld.superviseName},
        #{superviseInfoSubOld.resultTag},
        #{superviseInfoSubOld.resultValue},
        #{superviseInfoSubOld.score},
        #{superviseInfoSubOld.advice},
        #{superviseInfoSubOld.requst},
        #{superviseInfoSubOld.picTag},
        #{superviseInfoSubOld.remark}
        )
    </select>

</mapper>